<div>
    <h1>
        <%= title %>
    </h1>
    <p><%- messages() %></p>
    <section>
        <% if (locals.loggedin) { %>
            <h2> Welcome <%= accountData.account_firstname %></h2>
            <div class="update-profile">
                <a href="/account/profile-settings/<%= accountData.account_id %>">Update profile info</a>
            </div>
            <% if (isStaff){ %>
                <div class="inv-mgt">
                    <h3>Inventory Management</h3>
                    <a href="/inv/mgt/">Manage Inventory</a>
                </div>
            <%}%>
        <% } %>
        <div class=""></div>
    </section>
</div>